	"files": [
		{
			"content": "<script lang=\"ts\">\n\timport { cn } from \"$UTILS$\";\n\timport type { HTMLImgAttributes } from \"svelte/elements\";\n\n\texport type GeneratedImageLike = {\n\t\tbase64?: string;\n\t\tuint8Array?: Uint8Array;\n\t\tmediaType?: string;\n\t};\n\n\ttype Props = GeneratedImageLike & {\n\t\talt: string;\n\t\tclass?: string;\n\t} & Omit<HTMLImgAttributes, \"src\" | \"alt\" | \"class\">;\n\n\tlet {\n\t\tbase64,\n\t\tuint8Array,\n\t\tmediaType = \"image/png\",\n\t\tclass: className,\n\t\talt,\n\t\t...props\n\t}: Props = $props();\n\n\tlet src = $derived.by(() => {\n\t\t// Base64 takes precedence\n\t\tif (base64 && mediaType) {\n\t\t\treturn `data:${mediaType};base64,${base64}`;\n\t\t}\n\n\t\t// Handle Uint8Array\n\t\tif (uint8Array && mediaType) {\n\t\t\tconst blob = new Blob([uint8Array as BlobPart], { type: mediaType });\n\t\t\treturn URL.createObjectURL(blob);\n\t\t}\n\n\t\treturn undefined;\n\t});\n</script>\n\n{#if !src}\n\t<div\n\t\taria-label={alt}\n\t\trole=\"img\"\n\t\tclass={cn(\n\t\t\t\"h-auto max-w-full animate-pulse overflow-hidden rounded-md bg-gray-100 dark:bg-neutral-800\",\n\t\t\tclassName\n\t\t)}\n\t></div>\n{:else}\n\t<img\n\t\t{src}\n\t\t{alt}\n\t\tclass={cn(\"h-auto max-w-full overflow-hidden rounded-md\", className)}\n\t\t{...props}\n\t/>\n{/if}\n",
			"type": "registry:component",
			"target": "prompt-kit/image/Image.svelte"
		},
		{
			"content": "export { default as Image } from \"./Image.svelte\";\nexport type { GeneratedImageLike } from \"./Image.svelte\";\n",
			"type": "registry:file",
			"target": "prompt-kit/image/index.ts"
		}
	]
}