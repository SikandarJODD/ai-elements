{
  "$schema": "https://shadcn-svelte.com/schema/registry.json",
  "name": "Svelte AI Elements",
  "homepage": "https://ai-elements.vercel.app",
  "items": [
    {
      "name": "action",
      "type": "registry:block",
      "description": "The Actions component provides a flexible row of action buttons for AI responses with common actions like retry, like, dislike, copy, and share.",
      "files": [
        {
          "path": "src/lib/components/ai-elements/action/Action.svelte",
          "type": "registry:component",
          "target": "ai-elements/action/Action.svelte"
        },
        {
          "path": "src/lib/components/ai-elements/action/Actions.svelte",
          "type": "registry:component",
          "target": "ai-elements/action/Actions.svelte"
        },
        {
          "path": "src/lib/components/ai-elements/action/index.ts",
          "type": "registry:file",
          "target": "ai-elements/action/index.ts"
        }
      ],
      "registryDependencies": [
        "button",
        "tooltip"
      ],
      "meta": {
        "author": "SikandarJODD"
      }
    },
    {
      "name": "message",
      "type": "registry:block",
      "description": "The Message component displays a chat interface message from either a user or an AI. It includes an avatar, a name, and a message content.",
      "files": [
        {
          "path": "src/lib/components/ai-elements/message/Message.svelte",
          "type": "registry:component",
          "target": "ai-elements/message/Message.svelte"
        },
        {
          "path": "src/lib/components/ai-elements/message/MessageAvatar.svelte",
          "type": "registry:component",
          "target": "ai-elements/message/MessageAvatar.svelte"
        },
        {
          "path": "src/lib/components/ai-elements/message/MessageContent.svelte",
          "type": "registry:component",
          "target": "ai-elements/message/MessageContent.svelte"
        },
        {
          "path": "src/lib/components/ai-elements/message/index.ts",
          "type": "registry:file",
          "target": "ai-elements/message/index.ts"
        }
      ],
      "registryDependencies": [
        "avatar"
      ],
      "dependencies": [
        "ai",
        "tailwind-variants"
      ]
    }
  ]
}